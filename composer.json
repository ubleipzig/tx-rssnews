{
    "name": "ubl/rssnews",
    "type": "typo3-cms-extension",
    "description": "Extension for displaying external rss feeds at typo3 website",
    "homepage": "http://ub.uni-leipzig.de",
    "keywords":
            ["typo3", "rss", "extension"],
    "licence": ["MIT"],
    "support": {
		"issues": "https://github.com/ubleipzig/tx-rssnews/issues"
	},
	"require": {
		"typo3/cms": "^6.2 || ^7.6 || ^8.0"
	},
    "authors":[
        {
            "name": "Arun Chandran",
            "role": "Developer"    
        },
        {
             "name": "Minu Thomas",
             "role": "Developer"    
        },
        {
             "name": "Ulf Seltmann",
             "email": "ulf.seltmann@hmt-leipzig.de",
             "role": "Developer"    
        },
        {
             "name": "Frank Morgner",
             "email": "frank.morgner@uni-leipzig.de",
             "role": "Developer"                                                                                                                                                         
        }
    ],
    "autoload": {
        "psr-4": {
            "Pits\\PitsRssnews\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "rssnews"
        }
    }
}
